<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:id="@+id/_parentContainer">

    <ImageButton
            android:src="@android:drawable/ic_media_ff"
            android:layout_width="64dp"
            android:layout_height="64dp" android:id="@+id/_forwardButton"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="139dp" android:layout_marginStart="38dp"
            app:layout_constraintStart_toEndOf="@+id/_backButton" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="208dp"/>
    <ImageButton
            android:src="@android:drawable/ic_media_rew"
            android:layout_width="64dp"
            android:layout_height="64dp" android:id="@+id/_backButton"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="208dp" android:layout_marginStart="37dp"
            app:layout_constraintStart_toEndOf="@+id/_searchButton" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="139dp"/>
    <ImageButton
            android:src="@android:drawable/ic_menu_search"
            android:layout_width="64dp"
            android:layout_height="64dp" android:id="@+id/_searchButton"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="208dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="139dp" android:layout_marginStart="29dp"
            app:layout_constraintStart_toEndOf="@+id/_urlTextView"/>
    <EditText
            android:layout_width="191dp"
            android:layout_height="64dp"
            android:inputType="textPersonName"
            android:text="@string/url"
            android:ems="10"
            android:id="@+id/_urlTextView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="140dp"
            android:textSize="12dp" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="207dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>