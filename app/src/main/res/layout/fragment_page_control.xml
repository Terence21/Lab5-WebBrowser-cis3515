<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".PageControlFragment"
                                                   android:id="@+id/_parentContainer">
    <EditText
            android:layout_width="159dp"
            android:inputType="textUri"
            android:text="@string/url"
            android:ems="10"
            android:id="@+id/_urlTextView"
            android:textSize="12dp"
            android:layout_height="41dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="223dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="431dp" app:layout_constraintVertical_bias="0.276"/>
    <ImageButton
            android:src="@android:drawable/ic_menu_search"
            android:layout_width="40dp"
            android:layout_height="43dp" android:id="@+id/_searchButton"
            android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/_urlTextView" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="241dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="447dp" app:layout_constraintVertical_bias="0.298"/>
    <ImageButton
            android:src="@android:drawable/ic_media_rew"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/_backButton"
            android:layout_marginStart="52dp" app:layout_constraintStart_toEndOf="@+id/_searchButton"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="439dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="242dp"
            app:layout_constraintVertical_bias="0.3"/>
    <ImageButton
            android:src="@android:drawable/ic_media_ff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/_forwardButton"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="241dp" android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@+id/_backButton" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="439dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.062"
            app:layout_constraintVertical_bias="0.298"/>
</androidx.constraintlayout.widget.ConstraintLayout>